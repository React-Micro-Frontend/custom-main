# Environment Configuration Example
# Copy this file to .env for local development

# Node Environment
NODE_ENV=development

# API Configuration
REACT_APP_API_URL=http://localhost:3000/api

# Remote URLs (Development)
REACT_APP_USER_MGMT_URL=http://localhost:5001
REACT_APP_AUCTION_URL=http://localhost:5002
REACT_APP_LICENSE_URL=http://localhost:5003
REACT_APP_AUDIT_URL=http://localhost:5004

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_ERROR_TRACKING=false
